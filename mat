
1) General equation graph plot
	fplot('eqn' , [ , range])

2) Trignometric plot 
	range declaration { x = [0 : 2*pi] }
	when range  use ' . '
	ex ) y = x.*sin(x)
	plot(x,y)

3) r = sqrt(2*sin(5*t))
	x = r.*cos(t)
	y = r.*sin(t)
	polar(x,y) 

	FILL
	t = linspace(0,2*pi,1000)
 	r = sqrt(2*sin(5*t))
	x = r.*cos(t)
	y = r.*sin(t)
	fill(x,y,'r')

3)
	MESH
	x=(0:4)
	y(-4,4)
	[x,y] = meshgrid(x,y)
	z = (x.^2) - (y.^2)
	mesh(x,y,z)
	surf(x,y,z)
	contour(x,y,z)
	
